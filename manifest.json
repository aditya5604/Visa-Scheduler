{
    "manifest_version": 3,
    "name": "US Visa Appointment Scheduler",
    "description": "Under Development",
    "version": "1.0",
    "permissions": [
        "tabs",
        "alarms",
        "storage"
    ],
    "host_permissions": [
        "http://localhost:5000/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "visa_icon.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}
